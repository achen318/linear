>>> SWAP(row_a: 1, row_b: 1) <<<
-  -  -  --
1  1  1  -1
1  2  4   3
1  3  9   3
-  -  -  --

>>> MULT(scalar: 1.0, row: 1) <<<
-  -  -  --
1  1  1  -1
1  2  4   3
1  3  9   3
-  -  -  --

>>> SWAP(row_a: 1, row_b: 2) <<<
-  -  -  --
1  2  4   3
1  1  1  -1
1  3  9   3
-  -  -  --

>>> MULT(scalar: 1.0, row: 1) <<<
-  -  -  --
1  2  4   3
1  1  1  -1
1  3  9   3
-  -  -  --

>>> SWAP(row_a: 1, row_b: 3) <<<
-  -  -  --
1  3  9   3
1  1  1  -1
1  2  4   3
-  -  -  --

>>> MULT(scalar: 1.0, row: 1) <<<
-  -  -  --
1  3  9   3
1  1  1  -1
1  2  4   3
-  -  -  --

>>> SWAP(row_a: 2, row_b: 1) <<<
-  -  -  --
1  1  1  -1
1  3  9   3
1  2  4   3
-  -  -  --

>>> MULT(scalar: 0.3333333333333333, row: 2) <<<
--------  -  -  --
1         1  1  -1
0.333333  1  3   1
1         2  4   3
--------  -  -  --

>>> SWAP(row_a: 2, row_b: 2) <<<
--------  -  -  --
1         1  1  -1
0.333333  1  3   1
1         2  4   3
--------  -  -  --

>>> MULT(scalar: 1.0, row: 2) <<<
--------  -  -  --
1         1  1  -1
0.333333  1  3   1
1         2  4   3
--------  -  -  --

>>> SWAP(row_a: 2, row_b: 3) <<<
--------  -  -  --
1         1  1  -1
1         2  4   3
0.333333  1  3   1
--------  -  -  --

>>> MULT(scalar: 0.5, row: 2) <<<
--------  -  -  ----
1         1  1  -1
0.5       1  2   1.5
0.333333  1  3   1
--------  -  -  ----

>>> SWAP(row_a: 3, row_b: 1) <<<
--------  -  -  ----
0.333333  1  3   1
0.5       1  2   1.5
1         1  1  -1
--------  -  -  ----

>>> MULT(scalar: 1.0, row: 3) <<<
--------  -  -  ----
0.333333  1  3   1
0.5       1  2   1.5
1         1  1  -1
--------  -  -  ----

>>> SWAP(row_a: 3, row_b: 2) <<<
